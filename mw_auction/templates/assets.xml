<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--======================== Inclusion of Scss Files ======================-->
    <template id="theme_assets_mw_frontend" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/mw_auction/static/src/scss/style.scss"/>
            <link rel="stylesheet" type="text/scss" href="/mw_auction/static/src/scss/website_sale_backend.scss"/>

        </xpath>
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/mw_auction/static/src/js/frontend/shop.js"/>
            <script type="text/javascript" src="/mw_auction/static/src/js/frontend/theme_script.js"/>
            <script type="text/javascript" src="/mw_auction/static/src/js/frontend/website_profile.js"/>
            
        </xpath> 
    </template>

</odoo>
